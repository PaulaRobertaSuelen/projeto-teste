algoritmo "tabuadaparaatv"

var

   i, numero: inteiro

inicio

      escreval("----------")
      escreval("TABUADA")
      escreval("----------")
      escreval

      i <- 0
      escreva("Digite um número: ")
      leia(numero)
      escreval
      
      para i de 0 ate 10 faca
           escreval(numero, " x ", i, " = ", i*numero)
      fimpara

fimalgoritmo